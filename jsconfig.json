{
	"extends": "./.denoTypes/tsconfig.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext", "WebWorker"],
		"target": "esnext",
		"checkJs": true,
		"module": "esnext",
		"useDefineForClassFields": true,
		"strict": true,
		"exactOptionalPropertyTypes": true
	},
	"include": [
		"src/**/*.js",
		"scripts/**/*.js",
		"test/**/*.js",
		"test/fetchDependencies.test.js"
	]
}
